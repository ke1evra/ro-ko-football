{
  "name": "payload-starter",
  "version": "1.0.0",
  "description": "A saas starter template to get started with Payload 3.0",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "prebuild": "node scripts/patch-next-client-component-loader.cjs",
    "build": "next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "generate:api": "cross-env NODE_OPTIONS=--no-deprecation kubb",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix && pnpm run format",
    "lint:global": "eslint . --fix",
    "format": "prettier --cache --ignore-unknown --log-level warn --write .",
    "type-check": "tsc --noEmit --skipLibCheck --project tsconfig.typecheck.json",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start -p 3100",
    "pm2:start": "pm2 start ecosystem.config.cjs",
    "pm2:stop": "pm2 stop ecosystem.config.cjs",
    "pm2:restart": "pm2 restart ecosystem.config.cjs",
    "pm2:reload": "pm2 reload ecosystem.config.cjs",
    "pm2:delete": "pm2 delete ecosystem.config.cjs",
    "pm2:logs": "pm2 logs",
    "pm2:monit": "pm2 monit",
    "pm2:status": "pm2 status",
    "build:server": "next build",
    "production:check": "node scripts/production-check.js",
    "production:setup": "node scripts/production-setup.js",
    "db:push-to-prod": "chmod +x export-and-upload.sh && ./export-and-upload.sh",
    "db:apply-on-prod": "echo \"Run this on the server: chmod +x import-and-cleanup.sh && ./import-and-cleanup.sh\"",
    "deploy": "node deploy.mjs",
    "deploy:shell": "sh deploy.sh",
    "local-dev": "sh local-dev.sh",
    "start-all": "sh start-all.sh",
    "start-separate": "sh start-separate.sh",
    "env:setup": "node scripts/setup-env.js",
    "env:setup:interactive": "node scripts/setup-env.js --interactive",
    "env:setup:force": "node scripts/setup-env.js --force",
    "logos:test": "node scripts/test-download-logos.cjs",
    "logos:download": "node scripts/download-team-logos.cjs",
    "logos:status": "node scripts/check-logos-progress.cjs",
    "leagues:import": "node --loader @esbuild-kit/esm-loader scripts/import-leagues.mjs",
    "leagues:names:export": "node --loader @esbuild-kit/esm-loader scripts/export-league-names.mjs",
    "leagues:names:translate": "node scripts/translate-league-names.mjs",
    "leagues:names:translate:latest": "sh scripts/leagues-names-translate-latest.sh",
    "leagues:names:import": "node --loader @esbuild-kit/esm-loader scripts/import-league-names.mjs",
    "matches:import:forward": "tsx scripts/import-matches-forward.mjs",
    "matches:import:backward": "tsx scripts/import-matches-backward.mjs",
    "matches:import:forward:loop": "pnpm run matches:import:forward -- --loop --interval=600000",
    "matches:import:backward:loop": "pnpm run matches:import:backward -- --loop",
    "predictions:stats:calc": "tsx scripts/prediction-stats/calculate-all.mjs",
    "predictions:stats:calc:force": "tsx scripts/prediction-stats/calculate-all.mjs --force",
    "predictions:stats:calc:loop": "tsx scripts/prediction-stats/calculate-all.mjs --loop --interval=600000",
    "predictions:stats:recalc": "tsx scripts/prediction-stats/recalculate-all.mjs",
    "predictions:stats:by-match": "tsx scripts/prediction-stats/calculate-by-match.mjs",
    "predictions:stats:by-user": "tsx scripts/prediction-stats/calculate-by-user.mjs",
    "predictions:stats:by-post": "tsx scripts/prediction-stats/calculate-by-post.mjs"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp"
    ]
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@lexical/code": "0.28.0",
    "@lexical/link": "0.28.0",
    "@lexical/list": "0.28.0",
    "@lexical/markdown": "0.28.0",
    "@lexical/react": "0.28.0",
    "@lexical/rich-text": "0.28.0",
    "@lexical/selection": "0.28.0",
    "@lexical/table": "0.28.0",
    "@payloadcms/db-mongodb": "3.51.0",
    "@payloadcms/next": "3.51.0",
    "@payloadcms/payload-cloud": "3.51.0",
    "@payloadcms/richtext-lexical": "3.51.0",
    "@payloadcms/storage-s3": "3.51.0",
    "@payloadcms/storage-vercel-blob": "3.51.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/postcss": "^4.1.12",
    "@vercel/analytics": "^1.5.0",
    "chalk": "^5.6.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "cross-env": "^7.0.3",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.2",
    "embla-carousel-react": "^8.6.0",
    "graphql": "^16.11.0",
    "input-otp": "^1.4.2",
    "lexical": "0.28.0",
    "lucide-react": "^0.474.0",
    "next": "15.5.9",
    "next-themes": "^0.4.6",
    "payload": "3.51.0",
    "postcss": "^8.5.6",
    "react": "19.1.1",
    "react-day-picker": "^9.9.0",
    "react-dom": "19.1.1",
    "react-hook-form": "^7.62.0",
    "react-resizable-panels": "^3.0.5",
    "react-wrap-balancer": "^1.1.1",
    "recharts": "2.15.4",
    "resend": "^4.8.0",
    "sharp": "0.33.5",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^4.1.12",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.19.2",
    "vaul": "^1.1.2",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@esbuild-kit/esm-loader": "^2.6.5",
    "@eslint/eslintrc": "^3.3.1",
    "@kubb/core": "^3.18.3",
    "@kubb/plugin-client": "^3.18.3",
    "@kubb/plugin-oas": "^3.18.3",
    "@kubb/plugin-ts": "^3.18.3",
    "@next/eslint-plugin-next": "15.5.9",
    "@types/node": "^22.17.1",
    "@types/react": "19.0.7",
    "@types/react-dom": "19.0.3",
    "eslint": "^9.33.0",
    "eslint-config-next": "15.5.9",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-hooks": "^5.2.0",
    "kubb": "^3.0.105",
    "prettier": "^3.6.2",
    "tw-animate-css": "^1.3.6",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0"
  },
  "packageManager": "pnpm@10.15.1+sha512.34e538c329b5553014ca8e8f4535997f96180a1d0f614339357449935350d924e22f8614682191264ec33d1462ac21561aff97f6bb18065351c162c7e8f6de67"
}
